datasource db {
  provider             = "mysql"
  url                  = "mysql://root@127.0.0.1:3309/jssatu"
  referentialIntegrity = "prisma"
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

model Laporan {
  id    Int    @id @default(autoincrement())
  nomor String
}
